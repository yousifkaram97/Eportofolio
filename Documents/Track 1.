    "import random\n",
    "\n",
    "# Pick a random index\n",
    "idx = random.randint(0, X_test.shape[0]-1)\n",
    "sample_image = X_test[idx]\n",
    "sample_label = y_test[idx]\n",
    "\n",
    "# Extract HOG feature\n",
    "sample_feature = extract_hog_features([sample_image])\n",
    "\n",
    "# Predict using SVM and KNN\n",
    "pred_svm = svm_model.predict(sample_feature)[0]\n",
    "pred_knn = knn_model.predict(sample_feature)[0]\n",
    "\n",
    "# Show image and prediction\n",
    "plt.imshow(sample_image)\n",
    "plt.title(f\"True: {class_names[sample_label]}\\nSVM: {class_names[pred_svm]}, KNN: {class_names[pred_knn]}\")\n",
    "plt.axis('off')\n",
    "plt.show()\n"
